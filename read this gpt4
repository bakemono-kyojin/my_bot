def position_addtoshop_backp(remover, lang):
    #get_categories = get_all_categoriesx()
    if lang == 'ru':
        fwdbutton = "–î–∞–ª–µ–µ ‚û°"
        bwdbutton = "‚¨Ö –ù–∞–∑–∞–¥"
    if lang == 'en':
        fwdbutton = "Next ‚û°"
        bwdbutton = "‚¨Ö Back"
    get_shops = get_all_shopx()
