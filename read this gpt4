    if len(cm) > 0:
        print(f"Messages present:{str(touser_id)}")

    await dp.bot.send_message(
        chat_id=touser_id,
        text=f"–°–æ–æ–±—â–µ–Ω–∏–µ/–≤–æ–ø—Ä–æ—Å –ø–æ –∑–∞–∫–∞–∑—É –æ—Ç –ø–æ–∫—É–ø–∞—Ç–µ–ª—è:{messagetxt}",
        reply_markup=reply_order_message_finl(order_id),
    )

