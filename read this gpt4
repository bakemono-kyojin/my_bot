def menu_frep(user_id, lang):
    user_role = get_userx(user_id=user_id)['user_role']
    lang = get_userx(user_id=user_id)['user_lang']
    user_role = "User" if user_role is None else user_role

    if lang == 'ru':
        buybtn = "üéÅ –ö—É–ø–∏—Ç—å"
        sellbtn = "üåê –ü—Ä–æ–¥–∞—Ç—å"
        shopbtn = "üéÅ –ú–∞–≥–∞–∑–∏–Ω—ã"
        enbtn = "üè´ –ö—Ä—É–∂–∫–∏"
        entbtn = "–ê—Ñ–∏—à–∞"
        ptfbtn = "üë§ –ü—Ä–æ—Ñ–∏–ª—å"
        tubtn = "üí∞ –ü–æ–ø–æ–ª–Ω–∏—Ç—å"
        crtbtn = "üßÆ –ö–æ—Ä–∑–∏–Ω–∞"
        supbtn = "‚òé –ü–æ–¥–¥–µ—Ä–∂–∫–∞"
        isbtn = "–Ø –ø—Ä–æ–¥–∞–≤–µ—Ü"
        esbtn = "–ê–¥–º–∏–Ω –ê—Ñ–∏—à–∏"
        stabtn = "üìä –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞"
        prtbtn = "–ü–∞—Ä—Ç–Ω–µ—Ä—ã"
        pmbtn = "üéÅ –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ —Ç–æ–≤–∞—Ä–∞–º–∏ üñç"
        stbtn = "‚öô –ù–∞—Å—Ç—Ä–æ–π–∫–∏"
        embtn = "üé´ –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ —Å–æ–±—ã—Ç–∏—è–º–∏ üñç"
        ufbtn = "üîÜ –û–±—â–∏–µ —Ñ—É–Ω–∫—Ü–∏–∏"
        psbtn = "üîë –ü–ª–∞—Ç–µ–∂–Ω—ã–µ —Å–∏—Å—Ç–µ–º—ã"
        rsbtn = "–ó–∞–ø—Ä–æ—Å—ã –ø—Ä–æ–¥–∞–≤—Ü–æ–≤"
        obtn = "üöõ –ó–∞–∫–∞–∑—ã"
        srbtn = "üìä –û—Ç—á–µ—Ç –æ –ø—Ä–æ–¥–∞–∂–∞—Ö"

    if lang == 'en':
        buybtn = "üéÅ Buy"
        sellbtn = "üåê Sell"
        shopbtn = "üéÅ Shops"
        enbtn = "üè´ Cources"
        entbtn = "Events"
        ptfbtn = "üë§ Profile"
        tubtn = "üí∞ Top Up"
        crtbtn = "üßÆ Cart"
        supbtn = "‚òé Support"
        isbtn = "I'm seller"
        esbtn = "Events Admin"
        stabtn = "üìä Statistic"
        prtbtn = "Partners"
        pmbtn = "üéÅ Products Management üñç"
        stbtn = "‚öô Settings"
        embtn = "üé´ Events Management üñç"
        ufbtn = "üîÜ General Functions"
        psbtn = "üîë Payment Systems"
        rsbtn = "Sellers Request"
        obtn = "üöõ Orders"
        srbtn = "üìä Sales Report"

    keyboard = ReplyKeyboardMarkup(resize_keyboard=True)
    keyboard.row(buybtn, sellbtn)
    keyboard.row(shopbtn, entbtn)
    keyboard.row(ptfbtn, tubtn, crtbtn)

    if user_role is None or user_role == "":
        keyboard.row(supbtn, isbtn, esbtn, prtbtn)

    if user_role == "Admin": #in get_admins():
        keyboard.row(pmbtn, stabtn, prtbtn)
        keyboard.row(stbtn, embtn, ufbtn, psbtn)
        keyboard.row(rsbtn, obtn, srbtn)

    if user_role == "ShopAdmin":
        keyboard.row(supbtn, prtbtn)
        keyboard.row(pmbtn, psbtn)

    return keyboard

