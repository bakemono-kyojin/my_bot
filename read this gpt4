async def user_purchase_category_open(call: CallbackQuery, state: FSMContext):
    print('–û—Ç–∫—Ä—ã—Ç–∏–µ –º–∞–≥–∞–∑–∏–Ω–∞ –¥–ª—è –ø–æ–∫—É–ø–∫–∏ user_menu.py 133')
