    if len(get_positions) >= 1:
        await call.message.edit_text("<b>üéÅ –í—ã–±–µ—Ä–∏—Ç–µ –Ω—É–∂–Ω—ã–π –≤–∞–º —Ç–æ–≤–∞—Ä:</b>",
                                     reply_markup=products_shitem_position_open_fp(0, shop_id, city_id))
    else:
