                chlbtn,
                callback_data=f"position_edit_photo:{position_id}:{category_id}:{remover}",
            ),
            # –¥–æ–±–∞–≤–∏–ª 1.02.23    -----------------------------------------------------------
        )
        .add(
            ikb(
                chcbtn,
                callback_data=f"position_edit_city:{position_id}:{category_id}:{remover}",
