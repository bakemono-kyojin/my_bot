    city_id = get_city_user(user_id=user_id)

    await call.message.edit_text("<b>üéÅ –í—ã–±–µ—Ä–∏—Ç–µ –Ω—É–∂–Ω—ã–π –≤–∞–º —Ç–æ–≤–∞—Ä:</b>",
                                 reply_markup=buy_position_return_page_fp(remover, category_id, city_id))


