async def user_shop(message: Message, state: FSMContext):
    print('–û—Ç–∫—Ä—ã—Ç–∏–µ –∫–∞—Ç–µ–≥–æ—Ä–∏–π —Ç–æ–≤–∞—Ä–æ–≤  user_menu.py 65')
