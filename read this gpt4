                    save_balance.append(f"üá∞üáø –¢–µ–Ω–≥–µ: <code>{balance['balance']['amount']}‚Ç∏</code>")

            save_balance = "\n".join(save_balance)
            await self.dp.answer(f"<b>ü•ù –ë–∞–ª–∞–Ω—Å –∫–æ—à–µ–ª—å–∫–∞ <code>{self.login}</code> —Å–æ—Å—Ç–∞–≤–ª—è–µ—Ç:</b>\n"
                                 f"{save_balance}")

