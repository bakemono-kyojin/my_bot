tgbot >utilis>const_functions.py:
# - *- coding: utf- 8 - *-
import time
from datetime import datetime


# –û—á–∏—Å—Ç–∫–∞ —Ç–µ–∫—Å—Ç–∞ –æ—Ç HTML —Ç—ç–≥–æ–≤
def clear_html(get_text):
    if "<" in get_text: get_text = get_text.replace("<", "*")
    if ">" in get_text: get_text = get_text.replace(">", "*")

    return get_text


# –ü–æ–ª—É—á–µ–Ω–∏–µ —Ç–µ–∫—É—â–µ–≥–æ unix –≤—Ä–µ–º–µ–Ω–∏
def get_unix():
    return int(time.time())


# –ü–æ–ª—É—á–µ–Ω–∏–µ —Ç–µ–∫—É—â–µ–π –¥–∞—Ç—ã
def get_date():
    this_date = datetime.now().replace(microsecond=0)
    this_date = this_date.strftime("%d.%m.%Y %H:%M:%S")

    return this_date


# –†–∞–∑–±–∏–≤ —Å–ø–∏—Å–∫–∞ –ø–æ –∫–æ–ª–∏—á–µ—Å—Ç–≤—É –ø–µ—Ä–µ–¥–∞–Ω–Ω—ã—Ö –∑–Ω–∞—á–µ–Ω–∏–π
def split_messages(get_list, count):
    return [get_list[i:i + count] for i in range(0, len(get_list), count)]


# –û—á–∏—Å—Ç–∫–∞ –º—É—Å–æ—Ä–Ω—ã—Ö —Å–∏–º–≤–æ–ª–æ–≤ –∏–∑ —Å–ø–∏—Å–∫–∞
def clear_list(get_list: list):
    while "" in get_list:
        get_list.remove("")

    while " " in get_list:
        get_list.remove(" ")

    while "," in get_list:
        get_list.remove(",")

    while "\r" in get_list:
        get_list.remove("\r")

    return get_list


# –£–¥–∞–ª–µ–Ω–∏–µ –æ—Ç—Å—Ç—É–ø–æ–≤ —É —Ç–µ–∫—Å—Ç–∞
def ded(get_text: str):
    if get_text is not None:
        split_text = get_text.split("\n")
        if split_text[0] == "": split_text.pop(0)
        if split_text[-1] == "": split_text.pop(-1)
        save_text = []

        for text in split_text:
            while text.startswith(" "):
                text = text[1:]

            save_text.append(text)
        get_text = "\n".join(save_text)

    return get_text



# –ö–æ–Ω–≤–µ—Ä—Ç–∞—Ü–∏—è –¥–Ω–µ–π
def convert_day(day):
    day = int(day)
    days = ['–¥–µ–Ω—å', '–¥–Ω—è', '–¥–Ω–µ–π']

    if day % 10 == 1 and day % 100 != 11:
        count = 0
    elif 2 <= day % 10 <= 4 and (day % 100 < 10 or day % 100 >= 20):
        count = 1
    else:
        count = 2

    return f"{day} {days[count]}"

