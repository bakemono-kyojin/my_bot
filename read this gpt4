    profile_text += "‚ûñ‚ûñ‚ûñ‚ûñ‚ûñ‚ûñ‚ûñ‚ûñ‚ûñ‚ûñ\n"
    profile_text += _("üÜî ID: <code>", locale=lang) + str(get_user['user_id']) + "</code>\n"
